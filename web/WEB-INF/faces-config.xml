<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <locale-config>
            <default-locale>pt</default-locale>
        </locale-config>
        <message-bundle>Warnings.CustomMessages</message-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>timeHandler</managed-bean-name>
        <managed-bean-class>Beans.TimeHandler</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>jogadaHandler</managed-bean-name>
        <managed-bean-class>Beans.JogadaHandler</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
    </navigation-rule>
</faces-config>
